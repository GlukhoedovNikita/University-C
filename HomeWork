1.
   
#include<iostream>
using namespace std;
int main()
{
	setlocale(0, "ru");
	int W, H, R; // W и H - ширина и высота прямоугольного коврика, R – радиус запасного коврика

	cin >> W >> H >> R;

	if ((W<2*R)||(H < 2 * R)) cout << "NO"  << endl;
	else cout << "YES" << endl;

	system("Pause");
	return 0;
}

