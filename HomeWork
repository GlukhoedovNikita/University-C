12.

#include<iostream>
#include<algorithm>
#include<string>
#include<vector>
using namespace std;
int main()
{
	setlocale(0, "ru");
	string s;
	int k = 1, n = 0, res=0;
	cin >> s;
	vector<int> A(255);
	for (int i = 0; i < s.size(); ++i)
	{
		if (s[i] == '1')
		{
			k++;
			A.at(n) = k;
			n++;
		}
		else
		{
			k--;
			A.at(n) = k;
			n++;
		}
	}
	sort(begin(A), end(A));
	res = unique(begin(A), end(A)) - begin(A);
	cout << res << endl;
	system("Pause");
	return 0;
}



