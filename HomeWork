6.
#include<iostream>
#include<algorithm>
using namespace std;
int ma(int a, int b, int c)
{
	if (a > b&&a > c) return a;
	if (b > a&&b > c) return b;
	if (c > b&&c > a) return c;
	
}
int mi(int a, int b, int c)
{
	if (a < b&&a < c) return a;
	if (b < a&&b < c) return b;
	if (c < b&&c < a) return c;

}
int average(int a, int b, int c)
{
	if (mi(a, b, c) < a&&a < ma(a, b, c)) return a;
	if (mi(a, b, c) < b&&b < ma(a, b, c)) return b;
	if (mi(a, b, c) < c&&c < ma(a, b, c)) return c;
}
int main()
{
	setlocale(0, "ru");
	int a1, a2, a3, b1, b2, b3;
	cin >> a1 >> a2 >> a3 >> b1 >> b2 >> b3;
	cout << ma(a1, a2, a3)*ma(b1, b2, b3) + mi(a1, a2, a3)*mi(b1, b2, b3) + average(a1, a2, a3)*average(b1, b2, b3) << endl;
	system("Pause");
	return 0;
}

7.

#include<iostream>
using namespace std;
int main()
{
	setlocale(0, "ru");
	int k;
	char A[3] = { 'g','c','v'};
	cin >> k;
	
	for (int i = 1; i <=k; ++i)
	{
		char a, b;
		a = A[1];
		A[1] = A[2];
		A[2] = a;
		b = A[1];
		A[1] = A[0];
		A[0] = b;
	}
  
	for (int i = 0; i <= 2; ++i)
	{
		cout << A[i];
	}
  
	system("Pause");
	return 0;
}

