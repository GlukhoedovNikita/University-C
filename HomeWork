	system("Pause");
	return 0;
}

9.

#include<iostream>
bool bin(int n)
{
	for (int i = 1; n > 1; ++i)
	{
		if (n % 2 == 0) n = n / 2;
		else return false;
	}
	return true;
}
using namespace std;
int main()
{
	setlocale(0, "ru");
	int n;
	cin >> n;
	cout << (bin(n) ? "Yes":"No") << endl;

	system("Pause");
	return 0;
}

