#include<iostream>
using namespace std;
int main()
{
	setlocale(0, "ru");
	int N,b=0;
	cin >> N;

	for (int k = 1; k <= N; k++)
	{
		int a;
		cin >> a;
		if (a < 473)
		{
			cout << "Crash" << k << endl;
			b++;
			break;
		}
	}
	if(b==0) cout << "No Crash" << endl;

	system("Pause");
	return 0;
}

