#include<iostream>
using namespace std;
int main()
{
	setlocale(0, "ru");
	double L, W, H;   // L, W, H – длина, ширина и высота стен офиса
	cin >> L >> W >> H;

	cout << ceil(2 * (H*L + H * W) / 16 )<< endl;

	system("Pause");
	return 0;
}

